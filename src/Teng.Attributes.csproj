<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net8.0</TargetFrameworks>
    <Authors>Denuin</Authors>
    <Company>Denuin@live.com</Company>
    <Version>1.1.2</Version>
    <Copyright>Copyright Â© 2021-2024 Denuin</Copyright>
    <PackageProjectUrl>https://github.com/Denuin/Teng.Attributes</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Denuin/Teng.Attributes</RepositoryUrl>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageIcon>icon.png</PackageIcon>
    <PackageReadmeFile>README.md</PackageReadmeFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\icon.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>
</Project>